# Production Environment Variables for Memory Optimization
FLASK_ENV=production
DEBUG=false

# Memory optimization settings - CRITICAL for deployment
ENABLE_ENHANCED_RAG=false
RAG_SYNC_ON_START=false
DISABLE_RERANKER=true

# Server configuration
LOG_LEVEL=WARNING
RAG_TIMEOUT_SECONDS=30
OPENROUTER_TIMEOUT=30

# Add your actual keys (replace with real values):
OPENROUTER_API_KEY=your_openrouter_key_here
MONGO_URI=your_mongodb_uri_here
SECRET_KEY=your_secret_key_here

# Optional optimizations
CORS_ORIGINS=https://your-domain.com